<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_todo">

    <fragment
        android:id="@+id/navigation_todo"
        android:name="com.example.collegeschedulerapp.ui.todo.TodoFragment"
        android:label="@string/title_todo"
        tools:layout="@layout/fragment_todo" />

    <fragment
        android:id="@+id/navigation_exams"
        android:name="com.example.collegeschedulerapp.ui.exams.ExamsFragment"
        android:label="@string/title_exams"
        tools:layout="@layout/fragment_exams" />

    <fragment
        android:id="@+id/navigation_assignments"
        android:name="com.example.collegeschedulerapp.ui.assignments.AssignmentsFragment"
        android:label="@string/title_assignments"
        tools:layout="@layout/fragment_assignments" />
    <fragment
        android:id="@+id/navigation_courses"
        android:name="com.example.collegeschedulerapp.ui.courses.CoursesFragment"
        android:label="@string/title_courses"
        tools:layout="@layout/fragment_courses" />


</navigation>